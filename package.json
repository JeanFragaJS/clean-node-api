{
  "name": "clean-node-api",
  "version": "1.1.0",
  "main": "server.js",
  "author": "Jean Fraga <jeanfragoli395@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "ts-node --transpile-only src/main/server.ts",
    "test": "yarn test:unit && yarn test:functional  --coverage",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest --runInBand ",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts --fix",
    "prepare": "husky install",
    "style:check": "yarn prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "yarn prettier --write src/**/*.ts test/**/*.ts"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "3.3.14",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "git-commit-msg-linter": "^4.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "3.5.1",
    "nodemon": "^2.0.16",
    "sucrase": "^3.20.3",
    "validator": "^13.7.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
